event TimedEvent {
  time
};

event PositionEvent extends TimedEvent {
  position
};

event VelocityEvent {
  velocity
};

VelocityEvent calculateVelocity(PositionEvent posEv1, PositionEvent posEv2) := {
  velocity = (posEv2.time -  posEv1.time)^(-1) * (posEv2.position - posEv2.position)
};


VelocityRule : [PositionEvent, PositionEvent] -> calculateVelocity;
